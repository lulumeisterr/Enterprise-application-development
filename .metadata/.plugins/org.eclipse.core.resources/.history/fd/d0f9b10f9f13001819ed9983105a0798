package br.com.fiap.financias.util;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.EntityManager;

import br.com.fiap.financias.bean.Conta;

public class PopularConta {

	public static void main(String[] args) {

		EntityManager em = new JPAUtil().getEntityManager();

		for (int i = 0; i < 1; i++) {

			em.getTransaction().begin();

			Conta c = new Conta();

			c.setTitular("LuaN");
			c.setAgencia("104");
			c.setNumero("2");
			c.setBanco("Banco Itau");
			em.persist(c);
			
			c.setTitular("Maria");
			c.setAgencia("1");
			c.setNumero("3");
			c.setBanco("Banco Bradesco");
			em.persist(c);
			
			em.getTransaction().commit();

		}

		em.close();


	}





}


