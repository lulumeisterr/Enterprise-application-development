package br.com.fiap.bean;

import org.hibernate.annotations.Entity;

@Entity
public class Curso {
	
	@Id
	@Column(name ="cd_curso" , length=50 , nullable = false)
	@GeneratedValue(generator="curso" , strategy=GenerationType.SEQUENCE)
	private int codigo;
	private String nome;
	private int cargaHoraria;
	

}
