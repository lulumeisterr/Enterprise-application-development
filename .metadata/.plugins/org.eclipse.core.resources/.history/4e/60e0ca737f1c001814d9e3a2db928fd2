package br.com.fiap.bean;

import java.io.Serializable;
import java.util.Calendar;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

@Entity
@Table(name = "T_FUNCIONARIO")
@SequenceGenerator(name="T_FUNCIONARIO" , sequenceName = "SQ_TB_FUNCIONARIO" , allocationSize=1)

public class Funcionario implements Serializable {

	@GeneratedValue(generator="sequencia" ,  strategy = GenerationType.IDENTITY)
	@Id
	
	private int cdfuncionario;
	private String nm_funcionario;
	private Calendar dt_admissao;
	private double vl_salario;
	private byte[] foto;
	private EnumEscolaridade ds_escolaridade;
	
	
	
}
