public interface FuncionarioDAO {

	//Assinatura dos metodos
	
	void gravar(Funcionario f);
	
	void apagar(int codigo);
	
	void atualizar(Funcionario f);
	
	void commit()throws CommitException;
	
	Funcionario buscar(int codigo);
	
}
