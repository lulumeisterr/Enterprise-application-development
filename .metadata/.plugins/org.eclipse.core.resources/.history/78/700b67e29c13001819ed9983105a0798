package br.com.fiap.financias.util;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.EntityManager;

import br.com.fiap.financias.bean.Conta;

public class PopularConta {

	public static void main(String[] args) {
		
		
		List<Conta> lista = new ArrayList<>();
		
		Conta c = new Conta();
		
		c.setTitular("LuaN");
		c.setAgencia("104");
		c.setNumero("2");
		c.setBanco("Banco Itau");
		
		lista.add(c);
		
		c = new Conta();
		
		c.setTitular("Marcos");
		c.setAgencia("105");
		c.setNumero("3");
		c.setBanco("HSBC");
		
		lista.add(c);
		
		c = new Conta();
		
		c.setTitular("Suck");
		c.setAgencia("110");
		c.setNumero("9");
		c.setBanco("Banco do brasil");
		
		lista.add(c);
		
		
		EntityManager em = new JPAUtil().getEntityManager();
		
		em.getTransaction().begin();
		em.persist(lista.add(c));
		em.getTransaction().commit();
		
		em.close();
		
		
	}
	
}
