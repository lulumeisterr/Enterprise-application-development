package br.com.fiap.financias.util;

import java.util.ArrayList;
import java.util.List;

import javax.persistence.EntityManager;

import br.com.fiap.financias.bean.Conta;

public class PopularConta {

	public static void main(String[] args) {



		Conta c = new Conta();

		c.setTitular("LuaN");
		c.setAgencia("104");
		c.setNumero("2");
		c.setBanco("Banco Itau");


		for (int i = 0; i < 10; i++) {
			
		}


		EntityManager em = new JPAUtil().getEntityManager();

		em.getTransaction().begin();
		em.persist(c);
		em.getTransaction().commit();
		em.close();


	}





}

}
